{
  "name": "usako",
  "compatibility_date": "2025-04-22",
  "main": "src/server.ts",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "usako-messages",
      "database_id": "${DATABASE_ID}" // Cloudflare D1データベースIDを設定
    }
  ],
  "observability": {
    "enabled": true,
    "logs": {
      "invocation_logs": true
    }
  }
} 